# Module: performancemanager
# Docs: https://www.elastic.co/guide/en/beats/vspherebeat//vspherebeat-module-performancemanager.html

- module: performancemanager
  metricsets: ["virtualmachines", "hosts", "clusters", "datastores", "resourcepools", "datacenters", "vapps", "datastoresclusters"]
  enabled: false
  period: 20s
  maxQueries: 64
  hosts: ["localhost"]
  username: "username"
  password: "password"
